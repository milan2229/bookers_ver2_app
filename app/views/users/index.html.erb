<div class="container" px-5 px-sm-0>
	<div class="row">
		<div class="col-md-3">
			<%= render 'partial template/list', book: @book, user: @user %>
		</div>
		<div class="col-lg-9">
	<table class="table">
		<h2>Users</h2>
	<thead>
	<tr>
		<th>image</th>
 		<th>name</th>
 		<th></th>

 		</tr>
	</thead>
	<tbody>
		<% @users.reverse_each do |user| %>
		<tr>

			<td><%= attachment_image_tag user, :profile_image, format: 'jpeg', fallback: "no_image.jpg", size: "50x50" %></td>
			<td><%= user.name %></td>
			<td><%= link_to 'Show', user_path(user.id) %></td>
		</tr>
		<% end %>
	</tbody>
		     	</table>
		</div>
		<div class="col-md-8 offset-md-1">
		      <p style="padding-left:500px;">CopyRight Infratop.inc</p>
		</div>
  </div>
</div>