<!--Books画面-->

<h1>User info</h1>
<h1>New book</h1>



<% @books.each do |book| %>
  <tr>
  <td><%= book.title %></td>
  <td><%= book.body %></td>
  <td><%= link_to "Edit", edit_user_path(book.id) %></td>
  <td><%= link_to "Destroy", book_path(book.id), method: :delete %></td>
  </tr>
<% end %>




